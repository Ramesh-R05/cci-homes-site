{
  "name": "homes-automation",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ping": "bash -c \"./features/support/files/ping.sh\"",
    "ping:homes": "bash -c \"URL=homes-site.test.bxm.net.au npm run ping\"",
    "once": "bash -c \"./node_modules/.bin/chimp chimp.js --tags=@homes --tags=~@manual --browser=phantomjs --format=rerun:@rerun.txt\"",
    "test:mobile": "bash -c \"APP_KEY=homes-mobile npm run mobile\"",
    "test:browser": "bash -c \"APP_KEY=homes-browser npm run browsers\"",
    "mobile": "bash -c \"node runner.js --tags=@mobile --pMode=mobile\"",
    "test:high": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@high\"",
    "test:rerun": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs @rerun.txt \"",
    "debug": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --debug\"",
    "watch": "bash -c \"./node_modules/.bin/chimp chimp.js --watch\"",
    "ghost:watch": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --watch\"",
    "features": "bash -c \"node runner.js --pMode=features\"",
    "browsers": "bash -c \"node runner.js --tags=@browser\"",
    "homes:automation": "bash -c \"URL=http://homes-site.test.bxm.net.au/ APP_KEY=homes-site npm run test:high\"",
    "homes:watch": "bash -c \"URL=http://homes-site.test.bxm.net.au/ APP_KEY=homes-site npm run ghost:watch\"",
    "homes:stubbed": "bash -c \"export URL=http://localhost:3001 && export APP_KEY=homes-site && export APP_ENV=automation && npm run once\"",
    "smoketest": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@smoketest \"",
    "homes:smoketest": "bash -c \"URL=http://homes-site-au.sit.bxm.net.au/ npm run smoketest\"",
    "general:live": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --tags=@live \"",
    "all:test": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@homes --tags=~@cms --tags=~@manual\"",
    "test:cms": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@cms --tags=~@manual\"",
    "sit:au": "bash -c \"./node_modules/.bin/chimp chimp.shippable.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@sitAU\""
  },

  "dependencies": {
    "@bxm/automation": "4.0.3",
    "cucumber-html-report": "^0.2.8"
  }
}
